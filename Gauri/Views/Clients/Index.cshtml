@model List<Gauri.Models.ClientViewModel>

@{
    ViewBag.Title = "Lucrari";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Adauga lucrare", "Create")
</p>
<table>


@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Bloc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.D50)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.D60)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.D80)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.D112)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.D132)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReceivedAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Costs)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
